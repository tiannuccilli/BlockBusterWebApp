@{ViewData["Title"] = "Cities";
}
<h1>@ViewData["Title"]</h1>
<p>The following list are some of my favorite cities</p>
<ul>
    @{ foreach (var cities in ViewBag.Cities)
        {
            <li>@cities</li>
        }

    }
</ul>